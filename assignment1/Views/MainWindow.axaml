<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:assignment1.ViewModels" 
        x:Class="assignment1.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Title="Button Grid Example" Width="600" Height="400">

    <StackPanel>
        <Grid x:Name="ButtonContainer" />
        <TextBlock Text="{Binding Values}" Width="200" Height="30" Margin="10"/>
    </StackPanel>

</Window>
